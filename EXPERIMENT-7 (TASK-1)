    #include <stdio.h>

int main() {
    int array[100], size, c, largest;

    printf("Enter the number of elements in array\n");
    scanf("%d", &size);

    printf("Enter %d integer numbers\n", size);

    for (c = 0; c < size; c++) {
        scanf("%d", &array[c]);
    }

    largest = array[0];

    for (c = 1; c < size; c++) {
        if (array[c] > largest) {
            largest = array[c];
        }
    }

    printf("Largest element is *%d*\n", largest);

    return 0;
}
